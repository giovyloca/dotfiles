UXTerm*renderFont: true
UXTerm*faceName: Ubuntu Mono
UXTerm*faceSize: 11
UXTerm*termName: xterm-256color
UXTerm*background: #000000
UXTerm*foreground: #ffffff
*VT100*translations: #override Shift <Btn1Up>: exec-formatted("firefox '%t'", PRIMARY)

! special
*.foreground:   #a39e9b
*.background:   #1f1f1f
*.cursorColor:  #a39e9b

! black
*.color0:       #404040
*.color8:       #776e71

! red
*.color1:       #ef6155
*.color9:       #ef6155

! green
*.color2:       #48b685
*.color10:      #48b685

! yellow
*.color3:       #fec418
*.color11:      #fec418

! blue
*.color4:       #06b6ef
*.color12:      #06b6ef

! magenta
*.color5:       #815ba4
*.color13:      #815ba4

! cyan
*.color6:       #5bc4bf
*.color14:      #5bc4bf

! white
*.color7:       #a39e9b
*.color15:      #e7e9db

URxvt*renderFont: true
URxvt*faceName: Ubuntu Mono
URxvt*faceSize: 11
URxvt*termName: xterm-256color
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
